{
  "name": "frontend",
  "version": "0.0.1",
  "author": "Arif Sami",
  "scripts": {
    "start": "ng serve --port 4400",
    "start:es": "ng serve --configuration=es",
    "build:es": "ng build --configuration=production-es",
    "start:en": "ng serve --configuration=en",
    "build:en": "ng build --configuration=production-en",
    "start:hi": "ng serve --configuration=hi",
    "build:hi": "ng build --configuration=production-hi",
    "build": "ng build --prod --buildOptimizer",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run frontend:server:production",
    "ssr:deploy": "bash scripts/deploy.sh",
    "build:client-and-server-bundles-i18n": "ng run frontend:build:production-es && ng run frontend:build:production-en && ng run frontend:build:production-hi && ng run frontend:server:production-en && ng run frontend:server:production-hi && ng run frontend:server:production-es ",
    "build:i18n-ssr": "npm run build:client-and-server-bundles-i18n && npm run compile:server",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/localize": "^9.0.2",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@nguniversal/common": "^8.2.6",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "express": "^4.15.2",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.59",
    "codelyzer": "^5.0.0",
    "firebase-functions-test": "^0.1.6",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.10"
  }
}
